Host github.com
    ControlMaster auto
    ControlPersist 120

Host *
    ControlMaster auto
    ControlPath ~/.ssh/control/%r@%h:%p
    ControlPersist 1800
    ForwardX11 no
    ForwardAgent no

Host compute.*
    PubkeyAcceptedKeyTypes +ssh-rsa

